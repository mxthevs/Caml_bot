{
  "name": "caml_bot",
  "description": "Twitch IRC Bot built with OCaml",
  "author": {
    "name": "Matheus Henrique",
    "email": "mxthevsh@gmail.com",
    "url": "https://github.com/mxthevs"
  },
  "maintainers": [
    "Matheus Henrique"
  ],
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mxthevs/caml_bot"
  },
  "esy": {
    "sandboxEnv": {
      "PATH": "/opt/homebrew/bin:$PATH"
    },
    "build": [
      "dune build -p #{self.name}"
    ]
  },
  "scripts": {
    "watch": "dune build -p #{self.name} -w"
  },
  "dependencies": {
    "ocaml": "4.12.x",
    "@opam/dune": "*",
    "@opam/piaf": "0.1.0",
    "@opam/lwt": "5.4.2",
    "@opam/lwt_ppx": "2.0.2",
    "@opam/uuidm": "0.9.7",
    "@opam/caqti": "1.6.0",
    "@opam/caqti-lwt": "1.6.0",
    "@opam/caqti-driver-postgresql": "1.6.0",
    "@opam/ppx_rapper": "3.0.0",
    "@opam/ppx_rapper_lwt": "3.0.0"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "*",
    "@opam/ocamlformat": "*",
    "@opam/merlin": "*",
    "@opam/ocamlformat-rpc": "*",
    "@opam/utop": "*"
  },
  "resolutions": {
    "esy-openssl": "esy-packages/esy-openssl#619ae2d46ca981ec26ab3287487ad98b157a01d1",
    "@opam/conf-postgresql": "ManasJayanth/esy-postgres:esy.json#33cb05104f6c0eff27fab5c5317be773cb14c384"
  }
}
